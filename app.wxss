	border-color: #6192e6;
}
/*边框-辅色*/
.border-sub,
.border-sub .button,
.border-sub .tab-nav li a,
.border-sub .tab-body,
.button.bg-sub{
	border-color:#3f51b5;
}
/*边框-背景色*/
.border-back,
.border-back .button,
.border-back .tab-nav li a,
.border-back .tab-body,
.button.bg-back{
	border-color:#f3f3f5;
}
/*边框-融合色*/
.border-mix,
.border-mix .button,
.border-mix .tab-nav li a,
.border-mix .tab-body,
.button.bg-mix{
	border-color:#ffffff;
}
/*边框-点缀色*/
.border-dot,
.border-dot .button,
.border-dot .tab-nav li a,
.border-dot .tab-body,
.button.bg-dot{
	border-color:#f0b31f;
}
/*共用样式*/
page{font-size:28rpx;background-color:#fff}
.ellipsis,.text-ellipsis{white-space:nowrap;overflow:hidden;text-overflow:ellipsis}
.main{position:relative;display:block}
.main-index{padding-bottom:120rpx}
.footer{position:fixed;display:block;width:100%;height:120rpx;background-color:#f7f7f7;border-top:1px solid #e1e1e1;left:0;bottom:0;z-index:2}
.footer .icon{display:block;height:60rpx;line-height:60rpx;text-align:center;font-size:40rpx;padding-top:10rpx}
.footer .txt{display:block;height:50rpx;line-height:50rpx;font-size:24rpx;text-align:center}
.bitian{position:absolute;display:block;width:8px;height:25px;line-height:25px;color:red;left:-8px;top:0}
/*首页*/
.index-k{padding-top:42px}
.index-banner{position:relative;display:block;width:100%;height:200px}
.index-banner .index-banner-img{display:block;width:100%}
.index-search{position:fixed;display:block;height:30px;padding:5px 10px 7px 10px;left:0px;right:0px;top:0;z-index:2}
.index-search>input{position:relative;display:block;width:100%;height:30px;line-height:35px;background-color:rgba(255,255,255,.9);border-radius:5px;text-align:center}
.work-list{position:relative;display:block;padding:10px;border-bottom:1px solid #e1e1e1}
.work-pic{position:relative;display:block;width:95px}
.work-pic>image{display:block;width:100%;height:95px}
.work-pic>text{position:relative;display:block;width:100%;line-height:20px;padding-top:6px}
.work-txt{position:relative;display:block;line-height:15px;padding-top:5px;padding-bottom:5px;padding-left:15px}
.work-txt:first-child{padding-top:2px;padding-bottom:7px}
.work-footer{position:relative;display:block;width:100%}
.work-footer .icon{display:block;height:60rpx;line-height:60rpx;text-align:center;font-size:60rpx;padding-top:10rpx}
.work-footer .txt{display:block;height:50rpx;line-height:50rpx;text-align:center}
/*登录*/
.login-logo{position:relative;display:block;padding:50px 0}
.login-logo>image{display:block;width:100px;height:100px;border-radius:100%;border:1px solid #eee;margin:0 auto}
.login-int{position:relative;height:30px;line-height:30px;border-radius:40px;border:1px solid #ddd;padding:5px 5px}
.login-int input{position:relative;display:block;width:100%;height:30px;line-height:30px}
.login-int>.login-tips{position:absolute;display:block;height:15px;line-height:15px;padding:0 0;font-size:24rpx;color:#ccc;left:0;right:0;top:42px}
.login-btn{padding:10px 15px;border:none;border-radius:40px;box-shadow:0 0 10px #6192e6}
.bg-main-btn{padding:10px 15px;border:none;box-shadow:0 0 10px #6192e6}
/*个人中心*/
.home-avatar{position:relative;display:block;width:70px;height:70px}
.home-avatar>image{display:block;width:100%;height:100%;border-radius:100%}
/*促销*/
.work-int{position:relative;display:block;border:1px solid #ddd;border-radius:4px;padding:10px}
.work-int>textarea{display:block;width:100%;height:125px;line-height:25px}
.work-upload{position:relative;display:block;width:100%}
.work-upload>.work-upload-btn{position:relative;display:block;float:left;width:60px;height:60px;border:1px dashed #DDD;margin-top:10px;margin-right:10px}
.work-upload>.work-upload-btn>text{display:block;width:60px;height:60px;line-height:60px;font-size:24px;color:#CCC;text-align:center}
.work-upload>.work-upload-pic{position:relative;display:block;float:left;width:60px;height:60px;border:1px solid #eee;margin-top:10px;margin-right:10px}
.work-upload>.work-upload-pic>image{display:block;width:100%;height:100%}
.work-btn{padding:10px 15px;border:none;border-radius:40px;box-shadow:0 0 10px #6192e6}
.work-btn2{padding:10px 15px;border:none;border-radius:40px;box-shadow:0 0 10px #999999}
.work-cz{position:relative;display:block;width:60px;height:40px;line-height:40px;border-radius:100%;font-size:36px;text-align:center;color:#fff;margin:0 auto;padding:10px 0}
.work-cz.cz1{box-shadow:0 0 10px #89c997;background-color:#89c997}
.work-cz.cz2{box-shadow:0 0 10px #e3de99;background-color:#e3de99}
.work-cz.cz3{box-shadow:0 0 10px #88abda;background-color:#88abda}
.work-cz.cz4{box-shadow:0 0 10px #eb6877;background-color:#eb6877}
.work-cz.cz5{box-shadow:0 0 10px #00aebc;background-color:#00aebc}
.work-input{position:relative;display:block;min-height:25px;padding:10px}
.work-input-bt{position:relative;display:block;width:100px;height:25px;line-height:25px;color:#444}
.work-input-bt.long{width:150px}
.work-input-bd{display:block;width:100%;height:25px;line-height:25px}
.work-input-border{position:absolute;display:block;height:1px;background-color:#ddd;left:25px;right:-10px;bottom:0}
.work-select-bd{display:block;width:100%;height:25px;line-height:25px}
.work-picker{padding-right:20px}
.work-picker>.down{position:absolute;display:block;width:20px;height:45px;line-height:45px;text-align:right;right:0;top:0}
/*我的客户*/
.customer-search{position:fixed;height:30px;padding:5px 10px 7px 10px;left:0px;right:0px;top:0;z-index:2}
.customer-search input{position:relative;display:block;width:100%;height:30px;line-height:35px;background-color:rgba(255,255,255,.9);border-radius:5px;text-align:center}
.chaBtn{display:block;width:25px;height:25px;line-height:25px;color:#999;text-align:center;padding:0;font-size:14px}
.picker{line-height:25px}
label.radio{height:24px;line-height:24px}
/*促销执行查看*/
.carryout-k{padding-top:90px}
.carryout-k.s2{padding-top:48px}
.carryout-k2{padding-top:132px}
.carryout-k2.s2{padding-top:92px}
.carryout-search{position:fixed;display:block;width:auto;height:auto;padding:7px 10px;background-color:#fff;left:0;right:0;top:42px;z-index:2}
.carryout-search.s2{top:0}
.carryout-role{position:fixed;display:block;width:auto;height:auto;padding:7px 10px;padding-top:0;background-color:#fff;left:0;right:0;top:90px;z-index:2}
.carryout-role.s2{top:52px;}
.carryout-picker{position:relative;display:block;height:26px;padding:4px 10px;color:#999;text-align:center;background-color:#f2f2f2;border-radius:4px}
.work-details{position:relative;display:block;min-height:20px;line-height:20px;padding:10px}
.work-details-bt{position:relative;display:block;width:100px;min-height:20px;line-height:20px;color:#444}
.work-details-border{position:absolute;display:block;height:1px;background-color:#ddd;left:25px;right:-10px;bottom:0}
.work-details-pic{position:relative;width:75px;height:75px;margin-right:5px;border:1px solid #eee}
/*数据上报查看*/
.report-switch{position:fixed;display:block;width:100%;height:42px;background-color:#fff;text-align:center;left:0;top:0;z-index:2}
.report-switch text{display:inline-block;height:41px;line-height:42px;border-bottom:2px solid transparent}
.report-switch text.cur{color:#6192e6;border-bottom:2px solid #6192e6}
.work-tab{position:relative;display:block;width:100%;border:1px solid #ddd;color:#444;border-radius:10px;overflow:hidden}
.work-th{position:relative;display:block;width:100%;line-height:20px;background-color:#ddd}
.work-th text{display:block;padding:5px 10px}
.work-td{position:relative;display:block;width:100%;line-height:20px;border-top:1px solid #ddd}
.work-td text{display:block;padding:10px 10px}
